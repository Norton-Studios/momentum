{
  "name": "momentum",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "build": "react-router build",
    "dev": "docker compose up -d && react-router dev",
    "dev:server": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test -c e2e/playwright.config.ts",
    "test:e2e:db": "docker compose -f e2e/docker-compose.yml up -d --wait && npx prisma db push --skip-generate",
    "test:e2e:server": "yarn test:e2e:db && react-router dev",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@octokit/rest": "22.0.1",
    "@octokit/types": "16.0.0",
    "@prisma/client": "6.19.0",
    "@react-router/node": "7.9.6",
    "@react-router/serve": "7.9.6",
    "@tanstack/react-virtual": "^3.13.12",
    "bcrypt": "^6.0.0",
    "isbot": "5.1.32",
    "node-cron": "4.2.1",
    "p-graph": "1.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@playwright/test": "1.56.1",
    "@react-router/dev": "7.9.6",
    "@tailwindcss/vite": "4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/jsdom": "^27",
    "@types/node": "24.10.1",
    "@types/react": "19.2.6",
    "@types/react-dom": "19.2.3",
    "@vitest/coverage-v8": "4.0.13",
    "dotenv": "^17.2.3",
    "jsdom": "^27.2.0",
    "prisma": "6.19.0",
    "tailwindcss": "4.1.17",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vite": "7.2.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.13"
  }
}
